    "keywords": ["PHP","Router","PSR-7"],
    "description": "Collections Library & Interface",
    "licence":"MIT",
    "authors": [
        {
            "name": "Steven Miles",
            "email": "steve@kodcube.com",
            "homepage": "https://www.kodcube.com",
            "role": "Developer"
        }
    ],
    "require": {
      "php":">=7.0.0",
      "psr/log": ">=1.0.0",
      "kodcube/invoker": ">1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*",
        "zendframework/zend-diactoros": "^1.0"
    },
    "autoload": {
        "psr-4": {
             "KodCube\\Router\\":"./KodCube/Router"
        }
    },
